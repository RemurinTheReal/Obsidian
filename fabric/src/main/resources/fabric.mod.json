{
  "schemaVersion": 1,
  "id": "${mod_id}",
  "version": "${mod_version}",
  "name": "${mod_name}",
  "description": "${mod_description}",
  "authors": ["Remurin"],
  "contact": {
    "homepage": "${mod_url}",
    "sources": "${mod_sources_url}",
    "issues": "${mod_issues_url}"
  },
  "license": "${mod_license}",
  "icon": "assets/${mod_id}/icon.png",
  "environment": "client",
  "entrypoints": {
    "main": ["com.remurinthereal.obsidian.fabric.ObsidianFabric"],
    "client": ["com.remurinthereal.obsidian.fabric.client.ObsidianFabricClient"]
  },
  "mixins": ["${mod_id}.mixins.json"],
  "depends": {
    "fabricloader": ">=${fabric_loader_version}",
    "fabric-api": ">=${fabric_api_version}",
    "minecraft": ">=1.21.1",
    "java": ">=${java_version}"
  }
}